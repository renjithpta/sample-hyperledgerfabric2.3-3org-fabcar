    "@theledger/fabric-shim-crypto-types": "^1.0.4",
    "@types/express-serve-static-core": "4.0.49",
    "@types/node": "^8.5.8",
    "@types/winston": "^2.3.8",
    "@types/yup": "^0.24.1",
